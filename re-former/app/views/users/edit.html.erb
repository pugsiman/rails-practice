<%= form_for @user do |f| %>
  <%= render 'error_messages' %>
  <%= f.label :username, "Username: " %>
  <%= f.text_field :username %><br>
  <%= f.label :email, "Email: " %>
  <%= f.text_field :email, placeholder: "some@thing.com" %><br>
  <%= f.label :password, "Password: " %>
  <%= f.password_field :password %><br>
  <%= f.submit "Update" %>
<% end %>
